{
    "name": "aiida-jutools",
    "author": "The JuDFT Team",
    "author_email": "p.ruessmann@fz-juelich.de",
    "description": "Utilities for working with AiiDA as a user, and for JuDFTteam plugin developers",
    "url": "https://github.com/JuDFTteam/aiida-jutools",
    "license": "MIT",
    "classifiers": [
        "Programming Language :: Python :: 3",
        "Intended Audience :: Science/Research",
        "Topic :: Scientific/Engineering :: Physics",
        "License :: OSI Approved :: MIT License",
        "Natural Language :: English",
        "Framework :: AiiDA"
    ],
    "version": "0.1.0-dev1",
    "entry_points": {
    },
    "include_package_data": true,
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core>=1.1.0,<2.0.0",
        "sqlalchemy<1.4",
        "masci-tools",
        "numpy",
        "scipy",
        "pycifrw",
        "pymatgen",
        "spglib",
        "pandas",
        "humanfriendly"
    ],
    "extras_require": {
        "devtools": [
            "bump2version >= 1.0.0"
        ],
        "docs": [
            "sphinx",
            "sphinx-rtd-theme",
            "sphinx-autodoc-typehints",
            "sphinxcontrib-contentui"
        ],
        "pre-commit": [
            "astroid<2.5",
            "pre-commit~=2.2",
            "pylint~=2.5.0"
        ],
        "testing": [
            "pgtest~=1.3.1",
            "wheel~=0.31",
            "coverage",
            "pytest~=3.6,<5.0.0",
            "pytest-cov",
            "pytest-mpl",
            "pytest-timeout",
            "aiida-testing @ git+https://github.com/aiidateam/aiida-testing.git@40fbb3c564e250933653162ef5c56fda44348bd6"
        ]
    }
}
